//const size_t N = int(1e5)+1;

using namespace std;

int main(void){
    int t, n;
    cin >> t;
    while(t--){
        bool foundOne = false;
        cin >> n;
        int x, y=0;
        for(int i=0; i<n; ++i){
            cin >> x;
            if(x>1){
                foundOne = true;
            }
            y ^= x;
        }

        if((!y && foundOne) || (y==1 && !foundOne)){
            cout << "Brother"<<endl;
        }else{
            cout << "John"<<endl;
        }
    }
    return 0;
}
